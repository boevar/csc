<h1>Listing schedules</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Mwf1</th>
    <th>Mwf2</th>
    <th>Mwf3</th>
    <th>Tth1</th>
    <th>Tth2</th>
    <th>Tth3</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @schedules.each do |schedule| %>
  <tr>
    <td><%= schedule.name %></td>
    <td><%= schedule.mwf1 %></td>
    <td><%= schedule.mwf2 %></td>
    <td><%= schedule.mwf3 %></td>
    <td><%= schedule.tth1 %></td>
    <td><%= schedule.tth2 %></td>
    <td><%= schedule.tth3 %></td>
    <td><%= link_to 'Show', schedule %></td>
    <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
    <td><%= link_to 'Destroy', schedule, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Schedule', new_schedule_path %>
